<a class="navbar-brand site-name" href="/" title="Home">
  <img src="{% asset logo.svg @path %}" alt="{{ site.name }} Logo" height="32" width="32" itemprop="logo">
  {% assign siteNames = site.name | split: ' ' %}
  {% for siteName in siteNames %}
    <span>{{ siteName }}</span>
  {% endfor %}
</a>
